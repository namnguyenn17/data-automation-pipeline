# Example .env for Data Automation Pipeline (Yahoo Finance + Supabase)

# Supabase configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
SUPABASE_TABLE=stock_data

# Stock symbols to analyze (comma-separated)
STOCK_SYMBOLS=AMD,NVDA,BYD,TSLA

# Report output directory
REPORT_OUTPUT_DIR=reports

# Report formats (comma-separated: html,excel)
REPORT_FORMATS=html,excel

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/pipeline.log

# OPENAI
OPENAI_API_KEY=
OPENAI_MODEL=
